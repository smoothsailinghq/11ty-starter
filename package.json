{
  "name": "eleventy-starter",
  "version": "1.0.0",
  "type": "module",
  "description": "Boilerplate and a collection of components for Eleventy",
  "main": "src/assets/main.js",
  "scripts": {
    "start": "run-s dev",
    "dev": "run-p dev:* --print-label",
    "dev:eleventy": "eleventy --serve",
    "dev:vite": "vite",
    "debug": "DEBUG=Eleventy* npm start",
    "build": "NODE_ENV=production run-s clean build:vite build:eleventy clean:vite --print-label",
    "build:eleventy": "eleventy",
    "build:vite": "vite build",
    "clean": "del-cli dist",
    "clean:vite": "del-cli dist/.vite",
    "serve": "serve dist/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Dieter Peirs",
  "license": "MIT",
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Android version",
    "last 1 Firefox version",
    "last 1 FirefoxAndroid version",
    "last 1 Safari version",
    "last 1 iOS version",
    "last 1 Edge version"
  ],
  "dependencies": {
    "@11ty/eleventy": "^3.0.0-beta.1",
    "@11ty/eleventy-img": "^4.0.2",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@11ty/is-land": "^4.0.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.5",
    "del-cli": "^5.1.0",
    "eleventy-plugin-svg-sprite": "^2.4.2",
    "focus-trap": "^7.5.4",
    "html-minifier": "^4.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.44",
    "postcss-custom-media": "^10.0.8",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.0",
    "postcss-utopia": "^1.1.0",
    "svg-sprite": "^2.0.4",
    "vite": "^5.4.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-live-reload": "^3.0.3"
  }
}
